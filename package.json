{
  "name": "hal-json-vuex",
  "version": "3.0.0-alpha.2",
  "description": "Use a HAL JSON API with Vuex",
  "main": "./dist/hal-json-vuex.umd.cjs",
  "module": "./dist/hal-json-vuex.js",
  "typings": "./dist/hal-json-vuex.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hal-json-vuex.js",
      "require": "./dist/hal-json-vuex.umd.cjs"
    }
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc --noEmit && vite build",
    "build:watch": "cross-env NODE_ENV=production vite build --watch",
    "build-dev": "cross-env NODE_ENV=development tsc && vite build",
    "clean": "cross-env rimraf dist coverage lib",
    "coverage": "cross-env npm run build && vitest --coverage.enabled",
    "lint": "cross-env eslint src tests --ext .js,.ts",
    "test": "vitest",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest -i",
    "version": "npm run build"
  },
  "engines": {
    "node": ">=16.0.0 <21.0.0"
  },
  "type": "module",
  "files": [
    "/src",
    "/dist",
    "/CHANGELOG.md",
    "/LICENSE",
    "/package.json",
    "/README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecamp/hal-json-vuex.git"
  },
  "keywords": [
    "HAL",
    "JSON",
    "API",
    "HATEOAS",
    "Vuex",
    "Vue"
  ],
  "authors": [
    "Carlo Beltrame",
    "Urban Suppiger"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecamp/hal-json-vuex/issues"
  },
  "homepage": "https://github.com/ecamp/hal-json-vuex#readme",
  "dependencies": {
    "hal-json-normalizer": "^4.2.0",
    "url-template": "^3.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "@vitest/coverage-istanbul": "^2.0.5",
    "@vue/compiler-sfc": "3.3.6",
    "@vue/eslint-config-standard": "6.1.0",
    "@vue/test-utils": "2.4.6",
    "axios": "1.7.4",
    "axios-mock-adapter": "1.20.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "7.20.0",
    "lodash": "4.17.21",
    "mock-xmlhttprequest": "8.1.0",
    "rimraf": "3.0.2",
    "typescript": "4.5.5",
    "vite": "5.4.2",
    "vitest": "2.0.5",
    "vue": "3.3.6",
    "vuex": "4.1.0"
  }
}
